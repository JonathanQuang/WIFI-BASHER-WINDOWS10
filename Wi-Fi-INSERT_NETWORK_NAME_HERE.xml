<?xml version="1.0"?>
<WLANProfile xmlns="http://www.microsoft.com/networking/WLAN/profile/v1">
	<name>#PUT NAME OF WIFI NETWORK HERE#</name>
	<SSIDConfig>
		<SSID>
			<hex>#PUT NAME OF WIFI NETWORK CONVERTED TO HEX HERE #</hex>
			<name>#PUT NAME OF WIFI NETWORK HERE#</name>
		</SSID>
	</SSIDConfig>
	<connectionType>ESS</connectionType>
	<connectionMode>manual</connectionMode>
	<MSM>
		<security>
			<authEncryption>
				<authentication>WPA2PSK</authentication>
				<encryption>AES</encryption>
				<useOneX>false</useOneX>
			</authEncryption>
			<sharedKey>
				<keyType>passPhrase</keyType>
				<protected>false</protected>
				<keyMaterial>#PUT STARTING PASSWORD HERE, WILL BASH FROM LAST CHARACTER FIRST#</keyMaterial>
			</sharedKey>
		</security>
	</MSM>
</WLANProfile>
